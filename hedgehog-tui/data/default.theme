set statusbar.status.error fg:red
set statusbar.status.warning fg:yellow
set statusbar.status.information fg:blue
set statusbar.command.prompt fg:cyan

set list.item "fg:gray"
set list.item:focused "fg:white"
set list.item:selected "bg:gray fg:black"
set list.item:focused:selected "bg:white fg:black"
set list.item.missing +italic
